{
  "routes": [
    {
      "route": "/version.json",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate",
        "Pragma": "no-cache",
        "Expires": "0"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html"
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}
